                function ouvrirPage() {
                    var a = document.getElementById("search").value;

                    if (a === "accueil") {
                        window.open("/Users/romai/OneDrive/Bureau/HTML%20ECE/Projet%20Webdynamique/accueil.html");
                    }

                    if (a === "pornhub") {
                        window.open("https://fr.pornhub.com/video/search?search=granny+coquine");
                    }

                    if (a === "youtube") {
                        window.open("https://www.youtube.com/watch?v=dQw4w9WgXcQ");
                    }
                }
            </script>
  
